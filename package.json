{
  "name": "calenadarx",
  "version": "1.0.0",
  "description": "All in one calendar plugin",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently --kill-others \"npm run build:watch\" \"npm run dev-server\"",
    "dev-server": "node server.js",
    "build": "webpack",
    "build:watch": "webpack -w"
  },
  "dependencies": {
    "moment": "^2.13.0",
    "react": "^15.4.0",
    "react-dom": "^15.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.9.1",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.5.0",
    "concurrently": "^2.1.0",
    "css-loader": "^0.24.0",
    "eslint": "^3.10.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^3.8.0",
    "path": "^0.12.7",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SquadCalendar/squad.git"
  },
  "jest": {
    "verbose": false,
    "testRegex": "(/test/.*\\.spec\\.js)$",
    "moduleFileExtensions": [
      "jsx",
      "js"
    ]
  },
  "now": {
    "engines": {
      "node": "7.0.0"
    }
  },
  "author": "Squad",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SquadCalendar/squad/issues"
  },
  "homepage": "https://github.com/SquadCalendar/squad#readme"
}
