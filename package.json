{
  "name": "calendarx",
  "version": "1.2.0",
  "description": "React calendar component library",
  "main": "dist/calendarx.js",
  "umd:main": "dist/calendarx.umd.js",
  "module": "dist/calendarx.mjs",
  "source": "src/index.js",
  "scripts": {
    "test": "jest",
    "build": "microbundle --name Calendarx --jsx React.createElement --target web",
    "watch": "yarn build watch",
    "lint": "eslint src test",
    "format": "prettier {src,test}/*.js --single-quote --no-semi --print-width=100 --write",
    "prepublish": "yarn test && yarn build"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^16.8"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/react": "^8.0.1",
    "all-contributors-cli": "^6.6.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "jest": "^24.8.0",
    "microbundle": "^0.11.0",
    "moment": "^2.24.0",
    "prettier": "^1.17.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mfix22/calendarx.git"
  },
  "keywords": [
    "calendar",
    "plugin"
  ],
  "author": "Michael Fix",
  "bugs": {
    "url": "https://github.com/mfix22/calendarx/issues"
  },
  "homepage": "https://github.com/mfix22/calendarx#readme",
  "jest": {
    "clearMocks": true,
    "setupFilesAfterEnv": [
      "@testing-library/react/cleanup-after-each"
    ]
  }
}
